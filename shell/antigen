# Load antigen
source $HOME/.zsh-antigen/antigen.zsh

# Bundles
antigen bundles <<EOBUNDLES
  docker
  docker-compose
  command-not-found
  zpm-zsh/colorize
  zsh-users/zsh-autosuggestions
  zsh-users/zsh-completions
  zsh-users/zsh-syntax-highlighting
EOBUNDLES

# Use theme
antigen theme romkatv/powerlevel10k

# Apply all
antigen apply

